{
	"name": "@ga-ut/fetcher",
	"version": "0.1.0",
	"description": "A simple and powerful fetch wrapper with interceptors.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/ga-ut/fetcher",
	"repository": {
		"type": "git",
		"url": "https://github.com/ga-ut/fetcher.git"
	},
	"bugs": {
		"url": "https://github.com/ga-ut/fetcher/issues"
	},
	"scripts": {
		"build": "bun build.mjs",
		"deploy": "bun test && bun run build && npm publish --access public"
	},
	"keywords": [
		"fetch",
		"interceptor",
		"typescript",
		"http-client"
	],
	"author": "ga-ut",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"typescript": "^5.0.0",
		"@types/bun": "latest"
	},
	"files": [
		"dist"
	],
	"type": "commonjs",
	"sideEffects": false,
	"private": false,
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"default": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"packageManager": "bun@1.2.17"
}
